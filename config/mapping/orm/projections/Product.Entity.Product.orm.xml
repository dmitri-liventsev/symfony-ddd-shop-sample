<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
	<entity name="App\Infrastructure\Product\Entity\Product" table="product">
		<id name="uuid" type="uuid_binary" column="uuid"/>
		<field name="name" type="string" column="name"/>
		<field name="productsOnStock" type="integer" column="product_on_stock"/>
		<field name="price" type="integer" column="price"/>
		<field name="productType" type="string" length="50" column="product_type"/>
	</entity>
</doctrine-mapping>
